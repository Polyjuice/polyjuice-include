{
  "name": "starcounter-include",
  "version": "3.0.0-rc.2",
  "description": "Custom Element to include HTML partials/templates from Starcounter",
  "main": "starcounter-include.html",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "connect": "^3.6.3",
    "grunt": "^0.4.5",
    "grunt-bump": "^0.3.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-watch": "^0.6.1",
    "npm-run-all": "^4.1.1",
    "path": "^0.12.7"
  },
  "scripts": {
    "serve": "npm-run-all --parallel mockserver polyserve",
    "polyserve": "polyserve -o --proxy-target http://127.0.0.1:9999/sc --proxy-path sc",
    "test": "npm-run-all --parallel mockserver wct",
    "test:no-sauce": "npm-run-all --parallel mockserver wct:no-sauce",
    "test:sauce-only": "npm-run-all --parallel mockserver wct:sauce-only",
    "mockserver": "node test/mocked_sc_htmlmerger.js",
    "wct": "wct",
    "wct:no-sauce": "wct --skip-plugin sauce",
    "wct:sauce-only": "wct --skip-plugin local --plugin sauce"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Starcounter/starcounter-include.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Starcounter/starcounter-include/issues"
  },
  "homepage": "https://github.com/Starcounter/starcounter-include#readme"
}
