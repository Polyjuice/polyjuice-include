{
  "verbose": false,
  "plugins": {
    "local": {
      "browsers": ["chrome", "firefox"]
    },
    "sauce": {
      "browsers": [
        {
          "browserName": "Chrome",
          "platform": "Windows 10",
          "version": "71.0"
        },
        {
          "browserName": "Firefox",
          "platform": "Windows 10",
          "version": "64.0"
        }
      ]
    }
  },
  "webserver": {
    "hostname": "127.0.0.1",
    "port": "8080"
  },
  "proxy": {
    "path": "sc",
    "target": "http://127.0.0.1:9999/sc"
  }
}
